{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "launch cli",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/abacus.cli/bin/Debug/netcoreapp2.1/Abacus.CLI.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/abacus.cli",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "attach cli",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
